@isTest
private class BatchRenewalQuoteTest {
    
     BatchRenewalQuote job = new BatchRenewalQuote();
    
      @isTest static void createDirectOpportunity() {  
          
          Opportunity opp = new Opportunity;
          opp.Name = 'Test';
          opp.CloseDate  =  Date.today();
          opp.StageName = 'Prospecting';
          
          opp.Channel__c = 'Direct';
          
          insert opp;
          
          
      }
    
          @isTest static void createIndirectOpportunity() {  
          
          Opportunity opp2 = new Opportunity;
              
          opp2.Name = 'Test';
          opp2.CloseDate  =  Date.today();
          opp2.StageName = 'Prospecting';
          
          opp2.Channel__c = 'Indirect';
          
          insert opp2;
          
      }

    
      @isTest static void createQuoteDirect() {     
        
        SBQQ__Quote__c[] quoteListDirect = new List();
        for (Integer i=0;i<200;i++) {
            
		   SBQQ__Quote__c quote = new SBQQ__Quote__c();
           
           quote.SBQQ__Primary__c = True;
           quote.SBQQ__Opportunity2__c = opp.id;
           quote.SBQQ__Status__c = 'Draft';
           quote.SBQQ__Account__c = opp.AccountId;
           quote.Opportunity.Channel__c  = 'Direct';
            
            quoteListDirect.add(quote);
        }
        insert quoteListDirect;
          
          
      }
    
     @isTest static void createQuoteIndirect() {     
        
        SBQQ__Quote__c[] quoteListIndirect = new List();
        for (Integer i=0;i<200;i++) {
            
		   SBQQ__Quote__c quote2 = new SBQQ__Quote__c();
           
           quote2.SBQQ__Primary__c = True;
           quote2.SBQQ__Opportunity2__c = opp.id;
           quote2.SBQQ__Status__c = 'Draft';
           quote2.SBQQ__Account__c = opp.AccountId;
           quote2.Opportunity.Channel__c  = 'Indirect';
            
            quoteListIndirect.add(quote2);
        }
        insert quoteListIndirect;
          
          
      }

}
